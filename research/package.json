{
  "name": "open-ui-docs",
  "description": "Open UI documentation.",
  "version": "1.0.0",
  "author": "Open UI",
  "repository": {
    "type": "git",
    "url": "https://github.com/openui/open-ui"
  },
  "keywords": [
    "open ui"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "prettier --write",
      "git add"
    ],
    "**/*.json": [
      "prettier --parser json --write",
      "git add"
    ],
    "**/*.md": [
      "prettier --parser markdown --write",
      "git add"
    ],
    "**/*.mdx": [
      "prettier --parser mdx --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@astrojs/lit": "^0.2.0",
    "@astrojs/mdx": "^0.10.3",
    "@astrojs/react": "^0.1.3",
    "@astrojs/sitemap": "^0.2.1",
    "@astrojs/turbolinks": "^0.1.3",
    "astro": "^1.5.0",
    "eslint": "^8.18.0",
    "eslint-plugin-react": "^7.30.0",
    "prettier-plugin-astro": "^0.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@astropub/icons": "^0.2.0",
    "@emotion/core": "^10.0.28",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.15",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@nanostores/react": "^0.3.0",
    "anchor-js": "^4.3.1",
    "astro-icon": "^0.7.1",
    "husky": "^4.2.3",
    "json5-loader": "^4.0.1",
    "lint-staged": "^11.1.2",
    "lodash": "^4.17.21",
    "nanostores": "^0.6.0",
    "prettier": "^2.0.4",
    "prism-react-renderer": "^1.2.1",
    "prismjs": "^1.27.0",
    "prop-types": "^15.7.2",
    "react-helmet": "^6.1.0",
    "react-markdown": "^8.0.3",
    "react-typography": "^0.16.20",
    "remark-emoji": "^2.1.0",
    "remark-images": "^2.0.0",
    "typography": "^0.16.21"
  }
}
