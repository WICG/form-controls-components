{
  "name": "open-ui-docs",
  "description": "Open UI documentation.",
  "version": "1.0.0",
  "author": "Open UI",
  "repository": {
    "type": "git",
    "url": "https://github.com/openui/open-ui"
  },
  "keywords": [
    "open ui"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "prettier --write",
      "git add"
    ],
    "**/*.json": [
      "prettier --parser json --write",
      "git add"
    ],
    "**/*.md": [
      "prettier --parser markdown --write",
      "git add"
    ],
    "**/*.mdx": [
      "prettier --parser mdx --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@astrojs/mdx": "^0.11.6",
    "@astrojs/react": "^1.2.2",
    "@astrojs/sitemap": "^0.2.6",
    "astro": "^1.6.10",
    "eslint": "^8.18.0",
    "eslint-plugin-react": "^7.30.0",
    "prettier-plugin-astro": "^0.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@astrojs/prefetch": "^0.1.1",
    "@emotion/core": "^10.0.28",
    "@fontsource/cabin": "^4.5.9",
    "@fontsource/source-sans-pro": "^4.5.11",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.15",
    "@nanostores/react": "^0.4.0",
    "astro-compress": "^1.1.10",
    "astro-icon": "^0.7.3",
    "husky": "^4.2.3",
    "lint-staged": "^11.1.2",
    "lodash": "^4.17.21",
    "nanostores": "^0.7.1",
    "prettier": "^2.0.4"
  }
}
